{
  "name": "Blastflix",
  "version": "1.0.0",
  "description": "A NEtflix Electron wrapper for macOS",
  "productName": "Blastflix",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron src/main/main.js",
    "package": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/netflix.icns --prune=true --out=builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tommoor/macflix.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "netflix",
    "video"
  ],
  "author": "Alessio Biancalana",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dottorblaster/blastflix/issues"
  },
  "dependencies": {
    "jquery": "^2.1.4",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "electron-packager": "^8.6.0",
    "electron-prebuilt": "^0.36.0"
  }
}
